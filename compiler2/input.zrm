type Bool
{
    #cpp_forward("bool val;");
    #cpp_forward("#__Bool(bool val) { 
        this->val = val; 
    }");
}

fn Print(Bool x) 
{
    #cpp("std::cout << (#__x.val ? \"True\" : \"False\");");
}

fn Test(Bool a, Bool b)
{
    Print(a);
    Print(b);
}

fn Main() 
{
    Test(False, b: True);
}